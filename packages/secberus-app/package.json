{
  "name": "secberus-app",
  "version": "9.4.2",
  "private": true,
  "scripts": {
    "compile": "tsc --version && tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false",
    "build-workers": "webpack --config webWorker.config.js --mode production",
    "start": "DISABLE_ESLINT_PLUGIN=true REACT_APP_VERSION=$npm_package_version react-app-rewired start",
    "prebuild:run": "yarn build-workers",
    "build:deps": "time yarn workspace @secberus/icons build && time yarn workspace @secberus/utils build && time yarn workspace @secberus/services build && time yarn workspace @secberus/components build ",
    "build": "yarn build-workers && REACT_APP_VERSION=$npm_package_version TIMING=1 react-app-rewired build",
    "build-e2e": "USE_BABEL_PLUGIN_ISTANBUL=true yarn build",
    "build:run": "react-app-rewired build && serve -s build",
    "test": "npx playwright test",
    "test:debug": "PWDEBUG=1 npx playwright test --workers 1",
    "eject": "react-scripts eject",
    "reset": "rm -Rf node_modules && yarn cache clean -f && yarn install",
    "lint:styled": "stylelint './src/**/*.js'",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --cache --cache-location .eslintcache/",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --cache --cache-location .eslintcache/ --fix",
    "push": "rimraf ./licenses.csv && license-checker --out ./licenses.csv --csv && git add ./licenses.csv && yarn run test",
    "analyze": "node ./analyze.js",
    "e2e": "npx playwright test",
    "coverage": "npx nyc report --reporter=html --reporter=text-summary"
  },
  "dependencies": {
    "@analytics/segment": "^0.4.0",
    "@chakra-ui/react": "1.3.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@hookform/resolvers": "^1.0.0",
    "@reclaim-ai/react-intercom-hook": "^2.0.5",
    "@reduxjs/toolkit": "^1.8.0",
    "@secberus/components": "^9.4.2",
    "@secberus/icons": "^9.4.2",
    "@secberus/services": "^9.4.2",
    "@secberus/utils": "^9.4.2",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "analytics": "^0.4.0",
    "fetch-intercept": "^2.3.1",
    "fetch-retry": "^4.0.1",
    "framer-motion": "^3.3.0",
    "i18next": "^19.5.1",
    "immer": "^6.0.3",
    "lodash": "^4.17.21",
    "memoize-one": "^5.2.1",
    "moment": "^2.25.3",
    "path-to-regexp": "^6.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-hook-form": "^6.15.7",
    "react-i18next": "^11.7.0",
    "react-lazily": "^0.9.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.1",
    "react-to-print": "^2.10.0",
    "react-transition-group": "^4.3.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1",
